    "dev": "vite",
    "build": "npm run prebuild && vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "seo:validate": "node scripts/validate-seo.mjs",
    "seo:check": "npm run seo:validate",
    "veille:start": "node scripts/schedule-veille-articles.mjs",
    "veille:test": "node scripts/generate-veille-article.mjs",
    "veille:test-db": "node scripts/test-supabase-connection.mjs",
    "veille:test-scheduler": "node -e \"import('./scripts/schedule-veille-articles.mjs').then(m => new m.default().testGeneration())\"",
    "veille:stats": "cat logs/veille-stats.json",
    "veille:logs": "tail -f logs/veille-scheduler.log",
    "veille:errors": "cat logs/veille-errors.log",
    "build:sitemap-utils": "tsc src/utils/sitemapGenerator.ts src/data/calculators/calculatorsData.ts src/data/guides/guidesData.ts src/data/guides/additionalGuides.ts src/data/regulation/regulationData.ts src/data/regulation/additionalDTUs.ts --outDir build-sitemap-tmp/src --module NodeNext --target ES2020 --esModuleInterop --skipLibCheck --strict",
    "build:sitemap-script": "tsc scripts/scripts/generate-sitemap.ts --outDir build-sitemap-tmp --module NodeNext --target ES2020 --esModuleInterop --skipLibCheck --strict",
    "generate-sitemap": "node scripts/scripts/generate-sitemap.mjs",
    "clean:sitemap-tmp": "rm -rf build-sitemap-tmp",
    "prebuild": "npm run build:sitemap-utils && npm run build:sitemap-script && npm run generate-sitemap",
    "postbuild": "node scripts/scripts/generate-sitemap.mjs",
    "check-links": "node scripts/checkLinks.mjs",
    "generate-report": "ts-node scripts/generateReport.ts",
    "send-alert": "ts-node scripts/sendAlert.ts",
    "supabase:fix-security": "node scripts/configure-auth-security.mjs",
    "supabase:apply-migrations": "supabase db push"
